{
    "customDefaultAtt": {
        "type": "dateFile",
        "absolute": true,
        "alwaysIncludePattern": true
    },
    "appenders": [{
        "type": "console",
        "category": "console"
    }, {
        "pattern": "debug/yyyyMMdd.txt",
        "category": "logDebug"
    }, {
        "pattern": "info/yyyyMMdd.txt",
        "category": "logInfo"
    }, {
        "pattern": "warn/yyyyMMdd.txt",
        "category": "logWarn"
    }, {
        "pattern": "err/yyyyMMdd.txt",
        "category": "logErr"
    }],
    "replaceConsole": true,
    "levels": {
        "logDebug": "DEBUG",
        "logInfo": "DEBUG",
        "logWarn": "DEBUG",
        "logErr": "DEBUG"
    }
}