<%- include('common/top', {}); %>
<%- include('common/mainHeader', {}); %>
<div class="container">
    <div class="row ">
        <div class="col-xs-12 col-sm-8 col-md-6 col-sm-offset-2 col-md-offset-3 padding-0">
            <div class="border book-list-box login-box">
                <h2 class="book-list-title">登陆</h2>
                <form class="form-horizontal">
                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-2 control-label">用户名</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" id="username" name="username" placeholder="请输入用户名">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputPassword3" class="col-sm-2 control-label">密码</label>
                        <div class="col-sm-10">
                            <input type="password" class="form-control" id="userpass" name="userpass" placeholder="请输入密码">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputPassword3" class="col-sm-2 control-label">确认密码</label>
                        <div class="col-sm-10">
                            <input type="password" class="form-control" id="userpassqr" name="userpassqr" placeholder="请再次输入密码">
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-sm-offset-2 col-sm-10">
                            <button type="button" class="btn btn-default" onclick="register()">立即注册</button>
                            <span class="text_register"> 已有账号？<a id="goto_register" href="/login">立即登陆</a><br /></span>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
<%- include('common/footer', {}); %>
<script language="javascript" type="text/javascript" src="/static/src/js/login.js"></script>
<%- include('common/bottom', {}); %>